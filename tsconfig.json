{
  "compilerOptions": {
    "paths": {
      "@/*": ["./*"]
    },
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx"
, "components/ui/button.jsx", "components/ui/table.jsx", "lib/utils.js", "components/ui/input.jsx", "components/ui/dropdown-menu.jsx", "components/ui/checkbox.jsx"  ],
  "exclude": [
    "node_modules"
  ]
}
